#!/usr/bin/env python3

import sys
from platform import python_version
from pdfCrab import __main__

if python_version()[0:3] < '3.7':
    print('Python must be version 3.7 or higher\n')
    sys.exit()

if __name__ == '__main__':
    try:
        __main__.start()

    except Exception as _e:
        print('Broke in bin/pdfCrab\n\n{0}'.format(_e))
